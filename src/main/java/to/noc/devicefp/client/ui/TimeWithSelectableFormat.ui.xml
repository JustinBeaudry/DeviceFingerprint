<ui:UiBinder    xmlns:ui="urn:ui:com.google.gwt.uibinder"
                xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:style>
        .tzButton {
            color: #0b0b61;
            font-size: .85em;
            padding: .2em .4em .2em .4em;
        }
    </ui:style>

    <g:HTMLPanel>
        <span ui:field="timeDisplay"></span>
        <g:Button ui:field="tzButton" addStyleNames="{style.tzButton}">TZ</g:Button>                     
    </g:HTMLPanel>

</ui:UiBinder>
