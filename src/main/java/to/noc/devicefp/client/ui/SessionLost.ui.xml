<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
          
    <ui:style>        
        .reloadButton {
            margin: 1em;
        }        
    </ui:style>
    
    <g:DialogBox ui:field="dialogBox"
        autoHide="false"
        modal="true"
        glassEnabled="true"
        animationEnabled="true"
    >
       <g:caption>Session Lost</g:caption>
       <g:Button ui:field="reload" addStyleNames="{style.reloadButton}">Reload Page</g:Button>
    </g:DialogBox>

</ui:UiBinder>
