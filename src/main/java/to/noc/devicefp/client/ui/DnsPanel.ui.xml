<ui:UiBinder    xmlns:ui="urn:ui:com.google.gwt.uibinder"
                xmlns:g="urn:import:com.google.gwt.user.client.ui"
                >
    <ui:style>
    </ui:style>

    <g:HTMLPanel>
        <table>                       
            <tr>
                <td>Public DNS IP</td>
                <td><g:Label ui:field="publicDnsIpLabel"/></td>
            </tr>                            
            <tr>
                <td>Public DNS Host</td>
                <td><g:Label ui:field="publicDnsHostLabel"/></td>
            </tr>
            <tr>
                <td>Client did IPv6 Lookup</td>
                <td><g:Label ui:field="clientDidIPv6LookupLabel"/></td>
            </tr>                                      
            <tr ui:field="countryRow">
                <td>Server Country (conf%)</td>
                <td><g:Label ui:field="countryLabel"/></td>
            </tr>       
            <tr ui:field="regionRow">
                <td>Server Region (conf%)</td>
                <td><g:Label ui:field="regionLabel"/></td>
            </tr>              
            <tr ui:field="cityRow">
                <td>Server City (conf%)</td>
                <td><g:Label ui:field="cityLabel"/></td>
            </tr>                        
            <tr ui:field="ispRow">
                <td>Server ISP</td>
                <td><g:Label ui:field="ispLabel"/></td>
            </tr>  
            <tr ui:field="orgRow">
                <td>Server Organization</td>
                <td><g:Label ui:field="orgLabel"/></td>
            </tr>              
            <tr ui:field="asNumRow">
                <td>Server AS Number</td>
                <td><g:Label ui:field="asNumLabel"/></td>
            </tr>              
            <tr ui:field="domainRow">
                <td>Server Domain</td>
                <td><g:Label ui:field="domainLabel"/></td>
            </tr>   
            <tr ui:field="errorRow">
                <td>Error</td>
                <td><g:Label ui:field="errorLabel"/></td>
            </tr>               
        </table>         
    </g:HTMLPanel>
</ui:UiBinder>
